    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "compression": "^1.7.4",
    "dot-prop": "4.2.1",
    "express": "4.17.1",
    "express-http-proxy": "1.6.2",
    "lodash": "4.17.19",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "5.2.1",
